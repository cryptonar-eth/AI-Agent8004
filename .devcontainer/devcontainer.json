{
  "name": "AI-Agent8004 Codespace",
  "image": "mcr.microsoft.com/devcontainers/javascript-node:20",
  "features": {
    "ghcr.io/devcontainers/features/node:1": {
      "version": "20"
    }
  },
  "forwardPorts": [3000],
  "portsAttributes": {
    "3000": {
      "label": "A2A Server",
      "onAutoForward": "openBrowser",
      "protocol": "https"
    }
  },
  "postCreateCommand": "npm install",
  "remoteUser": "node"
}
